<%= form_for exercise_log do |f| %>
    <%= f.label :date %>
    <%= f.date_field :date %><br />
    <p>If you used no weights then put 0</p>
    <%= f.label :weights %>
    <%= f.text_field :weight %><br />
    <%= f.collection_select(:exercise_id, Exercise.all, :id, :name) %>
    <%= f.label :reps %>
    <%= f.text_field :reps %><br />
    <%= f.submit %>
<% end %>